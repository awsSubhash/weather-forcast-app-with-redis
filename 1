apiVersion: v1
kind: Service
metadata:
  name: my-new-app-service
spec:
  selector:
    app: app-redis
  type: NodePort
  ports:
    - name: http
      port: 80
      targetPort: 8080
      nodePort: 30009   # Must be between 30000-32767apiVersion: v1
